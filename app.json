{
  "name": "Heroku-v2ray",
  "description": "将v2ray部署在Heroku上",
  "keywords": [
    "v2ray VMess VLESS"
  ],
  "env": {
    "PROTOCOL": {
      "description": "协议VMess或VLESS，请填小写的vmess或vless",
      "value": "vless",
      "required": true
    },
    "UUID": {
      "description": "用户的主ID",
      "value": "9d2a5ca1-c799-4a4b-b769-b6c4e7a4dbea",
      "required": false
    },
    "WS_PATH": {
      "description": "websocket路径",
      "value": "/9d2a5ca1-c799-4a4b-b769-b6c4e7a4dbea-vless",
      "required": true
    }
  },
  "website": "https://github.com/GleasonMeggie/miss",
  "repository": "https://github.com/GleasonMeggie/miss",
  "stack": "container",
  "features": [
    "runtime-dyno-metadata"
  ]
}
